{
  "version": "1.0",
  "name": "Simple example",
  "ssl": true,
  "server": {
    "type": "hypercorn"
  },
  "endpoints": [
    {
      "prefix": "/",
      "match": "**/*",
      "backends": {
        "command": {
          "id": "docker",
          "darwin": "docker",
          "default": "docker"
        }
      },
      "upstream": {
        "proxy": {
          "enabled": true
        }
      }
    }
  ]
}
